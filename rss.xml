&lt;table&gt;
    &lt;thead&gt;
        &lt;tr&gt;
            &lt;th colspan=&quot;2&quot;&gt;&lt;/th&gt;
        &lt;/tr&gt;
    &lt;/thead&gt;
    &lt;tbody&gt;
        &lt;tr&gt;
            &lt;td&gt;&lt;b&gt;ER&lt;/b&gt;&lt;/td&gt;
            &lt;td rowspan=&quot;4&quot;&gt;&lt;b&gt;Reverse Triage&lt;/b&gt;&lt;br /&gt;
            Discharge or delay all non-critical patients &lt;br /&gt;
            in order to accommodate the new critical patients&lt;/td&gt;
        &lt;/tr&gt;
        &lt;tr&gt;&lt;td&gt;&lt;b&gt;OT&lt;/b&gt;&lt;/td&gt;&lt;/tr&gt;
        &lt;tr&gt;&lt;td&gt;&lt;b&gt;Wards&lt;/b&gt;&lt;/td&gt;&lt;/tr&gt;
        &lt;tr&gt;&lt;td&gt;&lt;b&gt;OPD&lt;/b&gt;&lt;/td&gt;&lt;/tr&gt;
